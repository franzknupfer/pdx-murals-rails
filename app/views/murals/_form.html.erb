<% render "layouts/errors", :object => @mural %>

<%= bootstrap_form_for @mural do |f| %>
  <%= f.text_field :mural_name %>
  <br>
  <%= f.text_field :year_created %>
  <br>
  <%= f.text_field :artist_name %>
  <%= f.text_field :artist_name2 %>
  <%= f.text_field :artist_name3 %>
  <%= f.text_field :mural_address %>
  <%= f.text_field :photo_credit %>
  <!-- <%= f.file_field :attached_image %> -->
  <br>
  <%= f.submit "Update", class: "btn btn-success" %>
<% end %>

<%= link_to "Return to home page", murals_path %>
